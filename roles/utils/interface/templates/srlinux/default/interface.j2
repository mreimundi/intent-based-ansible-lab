#jinja2: lstrip_blocks: "True"
{%- for itf, prop in interfaces.items() %}
- path: /interface[name={{ itf }}]
  value:
    admin-state: enable
    description: {{ prop.description|default('no description')}}
    {% if prop.vlan_tagging is defined %}
    vlan-tagging: true
    {% endif %}
{% endfor %}